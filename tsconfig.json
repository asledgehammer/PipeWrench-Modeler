{
  "version": "4.6.2",
  "compilerOptions": {
    "lib": [
      "ESNext",
      "DOM"
    ],
    "strict": false,
    "module": "commonjs",
    "jsx": "preserve",
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": false,
    "emitDecoratorMetadata": false,
    "experimentalDecorators": false,
    "outDir": "assets/js/",
    "rootDir": "src/ts/",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "skipLibCheck": true,
    "typeRoots": [
      "node_modules/@types",
      "node_modules/three/src/Three.d.ts",
      "node_modules/kmljs/src",
      "node_modules/pixi.js/pixi.js.d.ts",
      "node_modules/pixi-tilemap/dist/pixi-tilemap.d.ts",
      "node_modules/@types/stats/index.d.ts",
      "node_modules/monaco-editor/monaco.d.ts",
      "node_modules/highlightjs/lib/index.js"
    ],
    "strictNullChecks": false,
    "target": "ES6"
  },
  "files": [
    "src/ts/main.ts",
    "src/ts/headless.ts"
  ],
  "lib": [
    "es6",
    "dom",
  ]
}